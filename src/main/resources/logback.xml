<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%date{ss.SSS} %-3level | %msg%n</pattern>
    </encoder>
    <target>System.err</target>
  </appender>
  
  <root level="DEBUG">
    <appender-ref ref="CONSOLE"/>
  </root>

</configuration>